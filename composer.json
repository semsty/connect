{
  "name": "semsty/connect",
  "description": "service connect",
  "keywords": [
    "service connector"
  ],
  "type": "project",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-queue": "~2.0.2",
    "yiisoft/yii2-httpclient": "*"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1",
    "phpunit/phpunit": "~4.4"
  },
  "config": {
    "process-timeout": 1800
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "connect\\crm\\": "src",
      "connect\\crm\\base\\": "src/base",
      "connect\\crm\\amocrm\\": "src/amocrm",
      "connect\\crm\\bitrix24\\": "src/bitrix24",
      "connect\\crm\\custom\\": "src/custom",
      "connect\\crm\\retailcrm\\": "src/retailcrm",
      "connect\\crm\\tests\\": "tests",
      "connect\\crm\\tests\\base\\": "tests/base",
      "connect\\crm\\tests\\amocrm\\": "tests/amocrm",
      "connect\\crm\\tests\\bitrix24\\": "tests/bitrix24",
      "connect\\crm\\tests\\custom\\": "tests/custom",
      "connect\\crm\\tests\\retailcrm\\": "tests/retailcrm"
    }
  }
}
